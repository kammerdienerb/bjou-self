type foreachstmt extends astbase {
    ident : string
    expr  : astref
    stmts : astref[...]


    proc init(this) {
        astbase.init(this)
        this.ident = str()
        this.stmts = [...astref]
    }

    proc create() : foreachstmt {
        r := { foreachstmt: }
        r.init()
        return r
    }
}
