# floatliteral.bjou

type floatliteral extends exprbase {
    val : f64

    proc init(this, s : string ref) {
        exprbase.init(this)
        # @libc
        sscanf(s.c_str(), "%f", &this.val)
    }

    proc create(s : string ref) : floatliteral {
        r := { floatliteral:  }
        r.init(s)

        return r
    }
}
