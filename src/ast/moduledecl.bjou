# moduledecl.bjou

type moduledecl extends astbase {
    ident : string


    proc init(this) {
        astbase.init(this)
        this.ident = str()
    }

    proc create() : moduledecl {
        r := { moduledecl: }
        r.init()
        return r
    }

    proc visit(this, node : ast ref, visitor : <(ast ref, void*) : visit_action>, arg : void*) : visit_action {
        return visitor(node, arg)
    }
}
